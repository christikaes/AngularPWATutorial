{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/img/**",
          "/assets/sound/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "data",
      "urls": [
        "/assets/data.json"
      ],
      "cacheConfig": {
        "maxSize": 1,
        "maxAge": "1h",
        "strategy": "freshness"
      }
    }
  ]
}